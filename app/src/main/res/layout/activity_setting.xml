<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".SettingActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/back"
            android:layout_width="30dp"
            android:layout_height="56dp"
            android:layout_marginStart="@dimen/_7sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:padding="@dimen/_4sdp"
            android:src="@drawable/back_iso" />

        <TextView
            style="@style/headerTextStyle"
            android:layout_width="wrap_content"
            android:layout_height="56dp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:layout_toEndOf="@+id/back"
            android:gravity="center_vertical"
            android:text="Settings"
            android:textSize="@dimen/_12sdp" />

    </RelativeLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscriptions"
        style="@style/headerTextStyle"
        android:textColor="@color/blue1"
        android:textSize="@dimen/_11sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_20sdp"/>

    <TextView
        android:id="@+id/subscriptions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Subscriptions plans"
        style="@style/headerTextStyle"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_3sdp"/>

    <TextView
        android:id="@+id/subscriptions_tap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tap to see subscriptions plans"
        style="@style/textStyle"
        android:textColor="@color/gray"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/_15sdp"
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="@color/gray_text"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other"
        style="@style/headerTextStyle"
        android:textColor="@color/blue1"
        android:textSize="@dimen/_11sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_10sdp"/>

    <TextView
        android:id="@+id/feedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Feedback"
        style="@style/headerTextStyle"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_3sdp"/>

    <TextView
        android:id="@+id/feedback_tap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="If you have any suggestions, please contact us"
        style="@style/textStyle"
        android:textColor="@color/gray"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp" />

    <TextView
        android:id="@+id/rateUs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rate Us"
        style="@style/headerTextStyle"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_13sdp"/>

    <TextView
        android:id="@+id/rateUs_tap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Do you like this app? please support on playstore "
        style="@style/textStyle"
        android:textColor="@color/gray"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp" />

    <TextView
        android:id="@+id/share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share"
        style="@style/headerTextStyle"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_13sdp"/>

    <TextView
        android:id="@+id/share_tap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please share this app with your love ones"
        style="@style/textStyle"
        android:textColor="@color/gray"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/_15sdp"
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="@color/gray_text"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="About us"
        style="@style/headerTextStyle"
        android:textColor="@color/blue1"
        android:textSize="@dimen/_11sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_10sdp"/>

    <TextView
        android:id="@+id/moreApps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="More apps"
        style="@style/headerTextStyle"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_3sdp"/>

    <TextView
        android:id="@+id/moreApps_tap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tap to see our more apps"
        style="@style/textStyle"
        android:textColor="@color/gray"
        android:textSize="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_13sdp" />

</LinearLayout>